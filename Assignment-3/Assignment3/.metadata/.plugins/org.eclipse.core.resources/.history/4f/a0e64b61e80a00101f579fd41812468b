package com.app.geometry;
import java.util.Scanner;
import java.util.Arrays;


public class Point2D {
	private double x;
	private double y;
	
	public Point2D() {
		this.x = 0;
		this.y = 0;
	}
	
	public Point2D(double x , double y) {
		this.x = x;
		this.y = y;
	}
	
	public String getDetails() {
		String a = Arrays.toString(x);
		String b = Arrays.toString(y);
		
	}

}
